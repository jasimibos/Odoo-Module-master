<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="boec_custom_layout" inherit_id="website.layout" name="Custom Layout">
        <xpath expr="//div[@id='wrapwrap']/main" position="after">
            <html lang="en">
                <head>
                    <meta charset="UTF-8"/>
                    <meta name="description" content="Ogani Template"/>
                    <meta name="keywords" content="Ogani, unica, creative, html"/>
                    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
                    <meta http-equiv="X-UA-Compatible" content="ie=edge"/>
                    <title>Ogani | Template</title>

                    <link rel="stylesheet" href="/ibos_theme/static/src/css/bootstrap.min.css" type="text/css"/>
                    <link rel="stylesheet" href="/ibos_theme/static/src/css/font-awesome.min.css" type="text/css"/>
                    <link rel="stylesheet" href="/ibos_theme/static/src/css/elegant-icons.css" type="text/css"/>
                    <link rel="stylesheet" href="/ibos_theme/static/src/css/nice-select.css" type="text/css"/>
                    <link rel="stylesheet" href="/ibos_theme/static/src/css/jquery-ui.min.css" type="text/css"/>
                    <link rel="stylesheet" href="/ibos_theme/static/src/css/owl.carousel.min.css" type="text/css"/>
                    <link rel="stylesheet" href="/ibos_theme/static/src/css/slicknav.min.css" type="text/css"/>
                    <link rel="stylesheet" href="/ibos_theme/static/src/css/style.css" type="text/css"/>
                </head>
                <body class="">
                    <!-- Preloader -->
                    <div id="loader-wrapper">
                        <div class="loading" id="loading"></div>
                        <div class="loader-section section-left"/>
                        <div class="loader-section section-right"/>
                    </div>
                </body>
                <script type="text/javascript" src="/ibos_theme/static/src/js/jquery-3.3.1.min.js"/>
                <script type="text/javascript" src="/ibos_theme/static/src/js/bootstrap.min.js"/>
                <script type="text/javascript" src="/ibos_theme/static/src/js/jquery.nice-select.min.js"/>
                <script type="text/javascript" src="/ibos_theme/static/src/js/jquery-ui.min.js"/>
                <script type="text/javascript" src="/ibos_theme/static/src/js/jquery.slicknav.js"/>
                <script type="text/javascript" src="/ibos_theme/static/src/js/mixitup.min.js"/>
                <script type="text/javascript" src="/ibos_theme/static/src/js/owl.carousel.min.js"/>
                <script type="text/javascript" src="/ibos_theme/static/src/js/main.js"/>
            </html>
        </xpath>
    </template>
</odoo>