<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="theme_boec_assets" inherit_id="website.assets_frontend">
            <xpath expr="//link[last()]" position="after">

                <link href="/ibos_ecommerce/static/src/css/font-awesome.css" rel="stylesheet"/>
                <!-- Bootstrap -->
                <link href="/ibos_ecommerce/static/src/css/bootstrap.css" rel="stylesheet"/>
                <!-- SmartMenus jQuery Bootstrap Addon CSS -->
                <link href="/ibos_ecommerce/static/src/css/jquery.smartmenus.bootstrap.css" rel="stylesheet"/>
                <!-- Product view slider -->
                <link rel="stylesheet" type="text/css" href="/ibos_ecommerce/static/src/css/jquery.simpleLens.css"/>
                <!-- slick slider -->
                <link rel="stylesheet" type="text/css" href="/ibos_ecommerce/static/src/css/slick.css"/>
                <!-- price picker slider -->
                <link rel="stylesheet" type="text/css" href="/ibos_ecommerce/static/src/css/nouislider.css"/>
                <!-- Theme color -->
                <link id="switcher" href="/ibos_ecommerce/static/src/css/theme-color/default-theme.css"
                      rel="stylesheet"/>
                <!-- <link id="switcher" href="css/theme-color/bridge-theme.css" rel="stylesheet"> -->
                <!-- Top Slider CSS -->
                <link href="/ibos_ecommerce/static/src/css/sequence-theme.modern-slide-in.css" rel="stylesheet"
                      media="all"/>
                <!-- Main style sheet -->
                <link href="/ibos_ecommerce/static/src/css/style.css" rel="stylesheet"/>
                <!-- Google Font -->
                <link href='https://fonts.googleapis.com/css?family=Lato' rel='stylesheet' type='text/css'/>
                <link href='https://fonts.googleapis.com/css?family=Raleway' rel='stylesheet' type='text/css'/>
            </xpath>
            <xpath expr="//script[last()]" position="after">
                <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
                <!-- Include all compiled plugins (below), or include individual files as needed -->
                <script src="/ibos_ecommerce/static/src/js/bootstrap.js"></script>
                <!-- SmartMenus jQuery plugin -->
                <script type="text/javascript" src="/ibos_ecommerce/static/src/js/jquery.smartmenus.js"></script>
                <!-- SmartMenus jQuery Bootstrap Addon -->
                <script type="text/javascript"
                        src="/ibos_ecommerce/static/src/js/jquery.smartmenus.bootstrap.js"></script>
                <!-- To Slider JS -->
                <script src="/ibos_ecommerce/static/src/js/sequence.js"></script>
                <script src="/ibos_ecommerce/static/src/js/sequence-theme.modern-slide-in.js"></script>
                <!-- Product view slider -->
                <script type="text/javascript" src="/ibos_ecommerce/static/src/js/jquery.simpleGallery.js"></script>
                <script type="text/javascript" src="/ibos_ecommerce/static/src/js/jquery.simpleLens.js"></script>
                <!-- slick slider -->
                <script type="text/javascript" src="/ibos_ecommerce/static/src/js/slick.js"></script>
                <!-- Price picker slider -->
                <script type="text/javascript" src="/ibos_ecommerce/static/src/js/nouislider.js"></script>
                <!-- Custom js -->
                <script src="/ibos_ecommerce/static/src/js/custom.js"></script>
            </xpath>
            <xpath expr="." position="inside">
                <link href="https://fonts.googleapis.com/css2?family=Karla:ital,wght@0,200;0,300;0,400;0,500;0,600;0,700;0,800;1,300;1,500&amp;family=Montserrat&amp;display=swap"
                      rel="stylesheet"/>
            </xpath>
        </template>
    </data>
</odoo>